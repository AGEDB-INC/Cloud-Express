{
    "name": "ag-viewer",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "setup": "npm install && npm-run-all setup-front setup-backend",
        "setup-front": "cd frontend && npm install",
        "setup-backend": "cd backend && npm install",
        "front": "cd frontend && npm run start",
        "build-front": "cd frontend  && npm install && npm run build",
        "backend": "cd backend && npm run start",
        "deploy": "npm-run-all build-front backend",
        "start": "npm-run-all --parallel backend front",
        "agc-setup": "npm install && npm-run-all agc-setup-front setup-backend",
        "agc-setup-front": "cd agc-frontend && npm install",
        "agc-front": "cd agc-frontend && npm run start",
        "agc-build-front": "cd agc-frontend && npm install && npm run build",
        "agc-start": "npm-run-all --parallel backend agc-front"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5"
    }
}
